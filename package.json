{
  "name": "not-safe-not-fair",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dbclean": "npm run dbdrop && npm run dbcreate && npm run dbmu",
    "dbcreate": "npm run psql -- --dbname postgres -c 'CREATE DATABASE notsafenotfair'",
    "dbdrop": "npm run psql -- --dbname postgres -c 'DROP   DATABASE notsafenotfair'",
    "dbdump": "npm run pg_dump > dump.psql",
    "dbmc": "db-migrate create",
    "dbmu": "npm run docker -- node npx db-migrate up:all > migrations/log",
    "docker": "docker run -it --rm --network=netsafenetfair --env-file .env --mount type=bind,src=$PWD,dst=/nsnf -w /nsnf",
    "install:dev": "pip install --user aws-sam-cli",
    "pg_dump": "npm run docker -- postgres:9.6-alpine pg_dump",
    "posttest": "eslint --fix --ext .js,.jsx . && git diff --exit-code --text && npm run webpack:prod",
    "psql": "npm run docker -- postgres:9.6-alpine psql",
    "sam": "node_modules/aws-sam-local/node_modules/.bin/sam",
    "start": "npm run start:db && npm run start:api & npm run webpack:dev",
    "start:api": "node .env.js && npm run sam -- local start-api --docker-network=netsafenetfair --env-vars .env.json",
    "start:db": "docker-compose up -d",
    "stats": "npm run stats:client & npm run stats:server",
    "stats:client": "webpack --mode=production --config webpack.config.client.js --profile --json > stats.client.json",
    "stats:server": "webpack --mode=production --config webpack.config.server.js --profile --json > stats.server.json",
    "stop": "npm run stop:db",
    "stop:db": "docker-compose down",
    "test": "npm run start:db && npm run test:unit && sleep 3 && npm run dbclean && npm run docker -- node npm run test:db",
    "test:db": "mocha --grep @db",
    "test:unit": "mocha --grep @db --invert",
    "webpack:dev": "webpack --mode=development --watch",
    "webpack:prod": "webpack --mode=production"
  },
  "author": "aaronjameslang@googlemail.com",
  "license": "MIT",
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.42",
    "auth0-lock": "^11.6.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "cache-loader": "^1.2.0",
    "claudia-api-builder": "^4.0.0",
    "csv": "^3.0.2",
    "date-fns": "^1.29.0",
    "db-migrate": "^0.10.7",
    "db-migrate-pg": "^0.3.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.2.1",
    "jwk-to-pem": "^2.0.0",
    "lambda-local": "^1.4.7",
    "material-ui": "^1.0.0-beta.41",
    "mocha": "^5.1.1",
    "pg": "^7.4.3",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "snap-shot-it": "^5.0.0",
    "uuid": "^3.2.1",
    "webpack": "^4.8.1",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^2.1.3"
  }
}
